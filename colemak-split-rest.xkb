        };
        key <AD12> {[NoSymbol]};

    
        # Middle Alphabetic Row
        key <CAPS> {type="splitrest",
            symbols=[a,          A,          at,         NoSymbol,   at,                                     less],
            actions=[NoAction(), NoAction(), NoAction(), NoAction(), RedirectKey(key=<I175>,clearmods=Mod4), RedirectKey(key=<I163>,clearmods=Lock)]
        };
        key <AC01> {type="splitrest",
            symbols=[r,          R,          question,   2,                                      question,                               greater],
