        };
        key <AC09> {
            type="splitrest",
            symbols=[m,          M,          question,   plus,                                   NoSymbol,   bracketleft],
            actions=[NoAction(), NoAction(), NoAction(), RedirectKey(key=<I157>,clearmods=Mod3), NoAction(), RedirectKey(key=<I161>,clearmods=Lock)]
        };
        key <AC10> {
            type="splitrest",
            symbols=[comma,      underscore, NoSymbol,   Nosymbol,   NoSymbol,   bracketright],
            actions=[NoAction(), NoAction(), NoAction(), NoAction(), NoAction(), RedirectKey(key=<I162>,clearmods=Lock)]
        };

        key <AC11> {[Meta_L]};
