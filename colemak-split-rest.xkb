    # Used only for visualization of layout:
    xkb_geometry "common" {
        width = 290;
        height = 100;
        baseColor = "grey90";
        labelColor = "white";
    
        shape "NORM" { cornerRadius = 1, { [18, 18] }, { [2.5, 0], [15.5, 14] } };
