          	    };
          	};
          	row {
          	    top = 39;
          	    keys { { <CAPS>, "CAPS" },
          		   <AC01>, <AC02>, <AC03>, <AC04>, 
                 {<AC05>, color="grey80"}, {<AC06>, color="grey80"},
                 <AC07>, <AC08>, <AC09>, <AC10>, 
          		   <AC11>, <BKSL>
          	    };
          	};
          	row {
          	    top = 58;
          	    keys { { <LFSH>, "LFSH" }, <LSGT>,
